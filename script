                    for i,v in pairs(jumparguments) do
                        game.ReplicatedStorage.RemoteEvent:FireServer({[1] = v, [2] = 2})
                        wait(0.5)
                end
            end
        end
        end)
    
        JPAfkSection:NewButton("Afk Jump power 1T", "Automatically farms movement 1T", function(v)
            if ScriptActive then
                farmjumpactive = true
                farmfistactive = false
