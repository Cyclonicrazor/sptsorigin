                        wait(0.5)
                end
            end
        end
        end)
    
        JPAfkSection:NewTextBox("Input number", "Sends a discord notification when reached", function(txt)
            if ScriptActive then
                print("If you dont get it here are examples: 1290000000000, 12000000, 1000000000000000 Etc.")
                JPNumber = txt
            end
           end)
    
        local Discord = Window:NewTab("Discord")
        local DiscordSection = Discord:NewSection("Discord Notifications")
        DiscordSection:NewTextBox("Enter webhook", "Fill your webhook in for notifications!", function(txt)
            if ScriptActive then
                DiscordNotification = true
                webhookURL = txt
                print("Webhook saved!")
            end
        end)
    
        DiscordSection:NewButton("Send stat", "Put in number to send a notification when reached", function(v)
            if ScriptActive then
                if DiscordNotification then
                    local A_1 = game:GetService("Players").LocalPlayer.PrivateStats.FistStrength.Value
                    local A_2 = game:GetService("Players").LocalPlayer.PrivateStats.BodyToughness.Value
                    local A_3 = game:GetService("Players").LocalPlayer.PrivateStats.PsychicPower.Value
                    local A_4 = game:GetService("Players").LocalPlayer.PrivateStats.MovementSpeed.Value
                    local A_5 = game:GetService("Players").LocalPlayer.PrivateStats.JumpForce.Value
                    if FSNumber < A_1 then
                        DiscordOut("Your have reached your desired amount of Fist Strength: ".. A_1)
                    end
                    if BTNumber < A_2 then
                        DiscordOut("Your have reached your desired amount of Body Toughness: ".. A_2)
                    end
                    if PPNumber < A_3 then
                        DiscordOut("Your have reached your desired amount of Psychic Power: ".. A_3)
                    end
                    if MSNumber < A_4 then
                        DiscordOut("Your have reached your desired amount of Movement Speed: ".. A_4)
                    end
                    if JPNumber < A_5 then
                        DiscordOut("Your have reached your desired amount of Jump Force: ".. A_5)
                    end
                else
                    wait()
                end
            end
        end)
        DiscordSection:NewButton("Death message", "Sends a discord notification if you died", function(v)
            if ScriptActive then
                if DiscordNotification then
                    game:GetService("Players").LocalPlayer.Character.Humanoid.Died:connect(function()
                        DiscordOut("U have unfortunately died")
                    end)
                end
            end
        end)
    end
end
