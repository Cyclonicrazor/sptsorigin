        local JPAfk = Window:NewTab("Jump Power")
        local JPAfkSection = JPAfk:NewSection("Jump Power")
        JPAfkSection:NewButton("Afk Jump power <100LB", "Automatically farms movement <100LB", function(v)
            if ScriptActive then
                farmjumpactive = true
                farmfistactive = false
                farmbodyactive = false
                farmmovementactive = false
                PPUsed = false
                BTUsed = false
                FSUsed = false
                local A_2 = game:GetService("Players").LocalPlayer.Backpack.Weight
                A_2.Parent = game:GetService("Players").LocalPlayer.Character
                while farmjumpactive do
                    game:GetService('RunService').Stepped:wait()
                    jumparguments = {"Add_JF_Request"}
                    for i,v in pairs(jumparguments) do
                        game.ReplicatedStorage.RemoteEvent:FireServer({[1] = v, [2] = 1})
                        wait(0.5)
                end
            end
        end
        end)
    
        JPAfkSection:NewButton("Afk Jump power 100LB", "Automatically farms movement 100LB", function(v)
            if ScriptActive then
                farmjumpactive = true
                farmfistactive = false
                farmbodyactive = false
                farmmovementactive = false
                PPUsed = false
                BTUsed = false
                FSUsed = false
